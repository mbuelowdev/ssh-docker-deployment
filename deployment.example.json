{
  "version": "1.0.0",
  "name": "mbuelow.dev/my-project-p1",
  "tag": "latest",
  "url": "https://p1.mbuelow.dev",
  "informDiscord": true,
  "_documentation1": "The arguments are being passed to docker run. They can be empty.",
  "_documentation2": "You can mount custom stuff via the path /deployments/mbuelow_dev_my_project_p1/.",
  "_documentation3": "0.0.0.0 exposes the service publicly and 172.17.0.1 internally",
  "deployImages": [
    {
      "name": "mbuelow.dev/my-project-p1",
      "tag": "latest",
      "argPorts": "-p 172.17.0.1:42001:80",
      "argVolumes": "",
      "argExtra": "-e SOME_ENVIRONMENT_VAR_LOADED_FROM_SERVER_ENV",
      "argImageArgs": "--additional-arguments-to-the-image=true"
    },
    {
      "name": "postgres",
      "tag": "17.6",
      "argPorts": "-p 172.17.0.1:5432:5432",
      "argVolumes": "-v /deployments/mbuelow_dev_my_project_p1/init.sql:/docker-entrypoint-initdb.d/init.sql -v my_project_p1_postgres_data:/var/lib/postgresql",
      "argExtra": "-e POSTGRES_USER=user -e POSTGRES_PASSWORD=password -e POSTGRES_DB=database",
      "argImageArgs": ""
    }
  ]
}
